<?xml version="1.0" ?>
<sdf version="1.6">
  <world name="zero_gravity_world">
    <!-- Disable gravity -->
    <physics type="ode">
      <gravity>0 0 0</gravity>
    </physics>

    <!-- Add a light source -->
    <include>
      <uri>model://sun</uri>
    </include>

    <!-- Add ground plane (optional, remove if not needed) -->
    <include>
      <uri>model://ground_plane</uri>
    </include>
  </world>
</sdf>
